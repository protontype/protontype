{
  "port": "3000",
  "defaultRoutes": true,
  "database": {
    "name": "proton-example",
    "username": "",
    "password": "",
    "options": {
      "dialect": "sqlite",
      "storage": "./test/proton.sqlite",
      "define": {
        "underscored": "true"
      },
      "logging": false
    }
  },
  "cors": {
    "origin": ["*"],
    "methods": ["GET", "POST", "OPTIONS", "PUT", "PATCH", "DELETE"],
    "allowedHeaders": ["Content-Type", "Authorization"]
  },
  "jwtSecret": "secret",
  "jwtSession": {
    "session": false
  },
  "logger": {
    "enabled": false,
    "transports": [
      {
        "type": "file",
        "options": {
          "filename": "./test/logs.log"
        }
      },
      {
        "type": "console"
      }
    ]
  }
}